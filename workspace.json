{
  "version": 2,
  "projects": {
    "@bigcommerce/big-design": {
      "root": "packages/big-design"
    },
    "@bigcommerce/big-design-icons": {
      "root": "packages/big-design-icons"
    },
    "@bigcommerce/big-design-theme": {
      "root": "packages/big-design-theme"
    },
    "@bigcommerce/configs": {
      "root": "packages/configs"
    },
    "@bigcommerce/docs": {
      "root": "packages/docs",
      "targets": {
        "build": {
          "executor": "@nrwl/workspace:run-commands",
          "options": {
            "command": "yarn workspace @bigcommerce/docs run build"
          },
          "outputs": ["packages/docs/out", "packages/docs/.next"]
        },
        "typeCheck": {
          "executor": "@nrwl/workspace:run-commands",
          "options": {
            "command": "yarn workspace @bigcommerce/docs run typeCheck"
          }
        }
      }
    },
    "@bigcommerce/examples": {
      "root": "packages/examples"
    },
    "@bigcommerce/pack": {
      "root": "packages/pack"
    }
  }
}
